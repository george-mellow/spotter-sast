# Compliance Framework Exclusions by Environment
# Development Environment - Exclude strict government frameworks
@development:fisma,fedramp

# Test Environment - Exclude production-only frameworks  
@test:sox

# Staging Environment - Exclude some regulatory frameworks
@staging:fisma,fedramp

# File Type Exclusions for Specific Frameworks

# Exclude image files from most compliance scans
*.jpg:hipaa,gdpr,pci_dss
*.png:hipaa,gdpr,pci_dss
*.gif:hipaa,gdpr,pci_dss
*.svg:hipaa,gdpr,pci_dss

# Exclude font files
*.woff:all
*.woff2:all
*.ttf:all
*.eot:all

# Exclude compiled/minified files
*.min.js:all
*.min.css:all

